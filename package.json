{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "ci-test": "npm run typedtest",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "schema-ts": "npx openapi-typescript https://docs.building-energy-calculator.communities.gov.uk/openapi.yaml -o schema/api-schema.types.ts --path-params-as-types --make-paths-enum --root-types --enum",
    "test": "vitest --typecheck",
    "typecheck": "nuxt typecheck",
    "typedtest": "nuxt typecheck && vitest run",
    "e2e-test": "npx playwright test && rm -rf .nuxt/cache",
    "ci-e2e-test": "npx playwright test"
  },
  "dependencies": {
    "@formkit/nuxt": "^1.6.9",
