    "@sentry/nuxt": "^9.38.0",
    "ajv-formats": "^3.0.1",
    "aws-xray-sdk": "^3.10.3",
    "aws-xray-sdk-fetch": "^3.10.3",
    "dayjs": "^1.11.13",
    "deepmerge": "^4.3.1",
